@using EntityLayer.Concrete
@model List<Blog>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/UserLayout.cshtml";
}
@foreach (var item in Model)
{
<div class="col-md-12">
    <div class="blog-entry ftco-animate d-md-flex">
        <a href="single.html" class="img img-2" style="background-image: url(data:image/jpeg;base64,); "></a>
        <div class="text text-2 pl-md-4">
            <h3 class="mb-2"><a href="/Blog/BlogReadAll/@item.BlogID">@item.BlogTitle</a></h3>
            
            <div class="meta-wrap">
                <p class="meta">
                    <span><i class="icon-calendar mr-2"></i>@(((DateTime)item.BlogCreateDate).ToString("dd-MMM-yyyy"))</span>
                    <span><a href="single.html"><i class="icon-folder-o mr-2"></i>@item.Category.CategoryName</a></span>
                    <span><i class="icon-comment2 mr-2"></i>5 Comment</span>
                </p>
            </div>
            <p class="mb-4">@item.BlogContent.Substring(0,item.BlogContent.Substring(0,130).LastIndexOf(" "))...</p>
            <p><a href="/Blog/BlogReadAll/@item.BlogID" class="btn-custom">Daha Fazla <span class="ion-ios-arrow-forward"></span></a></p>
        </div>
    </div>
</div>
}