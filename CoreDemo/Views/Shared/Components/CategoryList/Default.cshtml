@model IEnumerable<EntityLayer.Concrete.Category>
<div class="sidebar-box ftco-animate">
    <h3 class="sidebar-heading">Kategoriler</h3>
    <ul class="categories">
        @foreach (var item in Model)
        {
        <li><a href="#">@item.CategoryName <span>(0)</span></a></li>
        }
    </ul>
</div>